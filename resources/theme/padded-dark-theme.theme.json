      "Button": {
        "hoverBackground": "table"
      },
      "Header": {
        "height": 42,
        "labelLeftRightInsets": "0,12,0,16",
        "toolbarLeftRightInsets": "0,12,0,8"
      },
      "HeaderTab": {
        "leftRightInsets": "0,12,0,12",
        "underlineArc": 4,
        "underlineHeight": 3,
        "verticalPadding": 6
      }
    },
